{
    "manifest_version": 2,

    "name": "Blurred Vines",
    "description": "This extension makes it easier to download an vine image.",
    "version": "1.0",
    "permissions": [
        "tabs", 
        "https://vine.co/*"
    ],
 //    "background": {
	// "scripts": ["background.js"]
 //    },
    "content_scripts": [
        {
            "matches": ["*"],
            "js": ["jquery.min.js", "drunkTyper.js"]
        }
        {
            "matches": ["https://vine.co/*"],
            "js": ["jquery.min.js", "inject.js"]
        }
    ],

    "web_accessible_resources": ["vine.css", "blurredvines.mp3", "Thickrolled.png"]

}

